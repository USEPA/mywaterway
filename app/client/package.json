    "prettier": "2.0.5"
  },
  "dependencies": {
    "@esri/react-arcgis": "5.1.0",
    "@gouch/to-title-case": "2.2.1",
    "@reach/dialog": "0.10.4",
    "@reach/router": "1.3.3",
    "@reach/tabs": "0.10.4",
    "@reach/window-size": "0.10.4",
    "eslint-config-react-app": "5.2.1",
    "esri-loader": "2.14.0",
    "glossary-panel": "github:Eastern-Research-Group/glossary",
    "highcharts": "8.1.2",
    "highcharts-react-official": "3.0.0",
    "react": "16.13.1",
    "react-app-polyfill": "1.0.6",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.1",
